}

<body class="hold-transition sidebar-mini">
    <div id="empLoader" class="loader-container">
        <span class="loader"></span>
    </div>
    <div class="row" style="padding-top: 51px;">
        <div class="col-md-12 mt-2" style="position:sticky">
            <nav class="navbar navbar-expand-sm schemaColor datatableHeaderStyle" style="background-color: #36454F; position: sticky; top: 160px; border-radius: 10px ;width:100%;height:45px">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" style="color:white; font-family:Tahoma">Booking List -</a>
                    </li>
                </ul>
                <ul class="navbar-nav ml-auto " id="BookUnitButton" style="display:none">
                    <li class="nav-item ">
                        <button type="button" data-toggle="modal" href="" data-target="#bookUnitModal" data-backdrop="static" data-keyboard="false" class="btn btn-primary " onclick="addBookedUnitBtn()" style="float:right ">Book Unit</button>
                    </li>
                </ul>
            </nav>
        </div>
    </div>


    @* Booking Application DATATABLE*@

